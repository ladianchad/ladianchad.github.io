\ProvidesPackage{TitledList}
\usepackage{CVSection} 

\NewDocumentCommand{\TitledListBegin}{m O{8pt} O{2pt}}{
  \noindent
  \parbox[t]{\linewidth}{
    #1
  }
  \CVSectionListBegin[#2][#3]
}

\newcommand{\TitledListEnd}{
  \CVSectionListEnd
}

\newcommand{\TitledListItem}[1]{
  \CVSectionListItem{#1}
}