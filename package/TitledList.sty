\ProvidesPackage{TitledList}
\usepackage{CVSection} 

\NewDocumentCommand{\TitledListBegin}{m O{12pt} O{2pt}}{%
  \par\noindent
  {\setlength{\parskip}{0pt}
   \parbox[t]{\linewidth}{#1}}%
  \noindent
  \CVSectionListBegin[#2][#3]
}

\newcommand{\TitledListEnd}{
  \CVSectionListEnd
}

\newcommand{\TitledListItem}[1]{
  \CVSectionListItem{#1}
}